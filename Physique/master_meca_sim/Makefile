ifndef config
  config=debug64
endif
export config

PROJECTS := shader_kit master_meca_sim_etudiant

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

shader_kit: 
	@echo "==== Building shader_kit ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f shader_kit.make

master_meca_sim_etudiant: 
	@echo "==== Building master_meca_sim_etudiant ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f master_meca_sim_etudiant.make

clean:
	@${MAKE} --no-print-directory -C . -f shader_kit.make clean
	@${MAKE} --no-print-directory -C . -f master_meca_sim_etudiant.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
