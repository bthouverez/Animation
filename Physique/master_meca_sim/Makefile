	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   shader_kit"
	@echo "   master_meca_sim_etudiant"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
